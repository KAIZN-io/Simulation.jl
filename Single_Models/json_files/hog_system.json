{
    "copa": {
        "signal_type": " signal_type",
        "single_impuls_NaCl": " NaCl_impuls",
        "t0": " NaCl_impuls_start",
        "t1": " t0 / 2 if signal_type == 3 else t0 + 300",
        "t2": " t0 / 2 if signal_type == 3 else 1"
    },
    "equation": {
        "NaCl": {
            "component": {
                "patNaCl1": "(0 if t < t0 else single_impuls_NaCl) if signal_type == 1 else ((single_impuls_NaCl if (t0 <= t and t < (t0 + t1)) else 0) if signal_type == 2 else ((single_impuls_NaCl if ((t - t0 - math.floor((t - t0) / (t1 + t2)) * (t1 + t2)) <= t1) else 0) if signal_type == 3 else ((max_NaCl if (0 if t < t0 else single_impuls_NaCl * math.ceil((t - t0) / t1)) > max_NaCl else (0 if t < t0 else single_impuls_NaCl * math.ceil((t - t0) / t1))) if signal_type == 4 else 0)))"
            },
            "unit": "mM"
        },
        "V_cell": {
            "component": {
                "patV_cell1": "V_os",
                "patV_cell2": "+V_b"
            },
            "unit": "fL"
        },
        "V_cyt": {
            "component": {
                "patV_cyt1": "0.5*V_cell"
            },
            "unit": "fL"
        },
        "V_nuc": {
            "component": {
                "patV_nuc1": "0.07*V_cell"
            },
            "unit": "fL"
        },
        "PI_e": {
            "component": {
                "patPI_e1": "PI_e0",
                "patPI_e2": "+w*NaCl"
            },
            "unit": "Pa"
        },
        "PI_t": {
            "component": {
                "patPI_t1": "max(PI_t0*(V_os-V_osPIt)/(V_os0-V_osPIt),0)"
            },
            "unit": "Pa"
        },
        "PI_i": {
            "component": {
                "patPI_i1": "((n_0+Glyc_in*V_cyt)/V_os)*R*T"
            },
            "unit": "Pa"
        },
        "V_ratio": {
            "component": {
                "patV_ratio1": "-G*Lp*(PI_e+PI_t-PI_i)/V_cell"
            },
            "unit": "s^-1"
        },
        "n_totalHog1": {
            "component": {
                "patn_totalHog11": "(Hog1c*V_cyt+Hog1n*V_nuc+Hog1PPc*V_cyt+Hog1PPn*V_nuc)"
            },
            "unit": "mM fL"
        },
        "totalHog1PP": {
            "component": {
                "pattotalHog1PP1": "(Hog1PPc*V_cyt+Hog1PPn*V_nuc)*602/6780"
            },
            "unit": "mM fL"
        }
    },
    "ODE": {
        "dGlyc_in": {
            "component": {
                "patGlyc_in1": "k_s0Glyc",
                "patGlyc_in2": "+((k_s1Glyc*totalHog1PP**4)/(beta**4+totalHog1PP**4))",
                "patGlyc_in3": "+k_s2Glyc*Yt",
                "patGlyc_in4": "-(k_exp0Glyc+((k_exp1Glyc*PI_t**hill_exponent_eflux)/(gamma**hill_exponent_eflux+PI_t**hill_exponent_eflux)))*(Glyc_in-Glyc_ex)",
                "patGlyc_in5": "-Glyc_in*V_ratio"
            },
            "unit": "mM"
        },
        "dHog1PPc": {
            "component": {
                "patHog1PPc1": "k_phoHog1*Pbs2PP*Hog1c",
                "patHog1PPc2": "-k_dephoHog1PPc*Hog1PPc",
                "patHog1PPc3": "-k_impHog1PPc*Hog1PPc",
                "patHog1PPc4": "+((k_expHog1PPn*Hog1PPn*V_nuc)/V_cyt)",
                "patHog1PPc5": "-Hog1PPc*V_ratio"
            },
            "unit": "mM"
        },
        "dHog1PPn": {
            "component": {
                "patHog1PPn1": "((k_impHog1PPc*Hog1PPc*V_cyt)/V_nuc)",
                "patHog1PPn2": "-k_expHog1PPn*Hog1PPn",
                "patHog1PPn3": "-k_dephoHog1PPn*Hog1PPn",
                "patHog1PPn4": "-Hog1PPn*V_ratio"
            },
            "unit": "mM"
        },
        "dHog1c": {
            "component": {
                "patHog1c2": "-k_phoHog1*Pbs2PP*Hog1c",
                "patHog1c3": "+k_dephoHog1PPc*Hog1PPc",
                "patHog1c4": "-k_impHog1c*Hog1c",
                "patHog1c5": "+((k_expHog1n*Hog1n*V_nuc)/V_cyt)",
                "patHog1c6": "-Hog1c*V_ratio"
            },
            "unit": "mM"
        },
        "dHog1n": {
            "component": {
                "patHog1n1": "((k_impHog1c*Hog1c*V_cyt)/V_nuc)",
                "patHog1n2": "-k_expHog1n*Hog1n",
                "patHog1n3": "+k_dephoHog1PPn*Hog1PPn",
                "patHog1n4": "-Hog1n*V_ratio"
            },
            "unit": "mM"
        },
        "dPbs2": {
            "component": {
                "patPbs21": "-((k_phoPbs2*Pbs2)/(1+(PI_t/alpha)**hill_exponent))",
                "patPbs22": "+k_dephoPbs2*Pbs2PP",
                "patPbs23": "-Pbs2*V_ratio"
            },
            "unit": "mM"
        },
        "dPbs2PP": {
            "component": {
                "patPbs2PP1": "((k_phoPbs2*Pbs2)/(1+(PI_t/alpha)**hill_exponent))",
                "patPbs2PP2": "-k_dephoPbs2*Pbs2PP",
                "patPbs2PP3": "-Pbs2PP*V_ratio"
            },
            "unit": "mM"
        },
        "dV_os": {
            "component": {
                "patV_os2": "-G*Lp*(PI_e+PI_t-PI_i)"
            },
            "unit": "fL"
        },
        "dYt": {
            "component": {
                "patYt1": "k_s0Yt",
                "patYt2": "+k_s1Yt*z4",
                "patYt3": "-k_tYt*Yt",
                "patYt4": "-Yt*V_ratio"
            },
            "unit": "mM"
        },
        "dz1": {
            "component": {
                "patz11": "((4*(Hog1PPn-z1))/tau)"
            },
            "unit": "mM"
        },
        "dz2": {
            "component": {
                "patz21": "((4*(z1-z2))/tau)"
            },
            "unit": "mM"
        },
        "dz3": {
            "component": {
                "patz31": "((4*(z2-z3))/tau)"
            },
            "unit": "mM"
        },
        "dz4": {
            "component": {
                "patz41": "((4*(z3-z4))/tau)"
            },
            "unit": "mM"
        }
    }
}