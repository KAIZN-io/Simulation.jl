{
    "copa": {
        "t0_glucose": " Glucose_impuls_start",
        "t1_glucose": " Glucose_impuls_end",
        "Glucose": " glucose_switch[0]"
    },
    "equation": {
        "c_e": {
            "component": {
                "patc_e1": "H_out",
                "patc_e2": "+K_out",
                "patc_e3": "+Cl_out",
                "patc_e4": "+Na_out",
                "patc_e5": "+Sorbitol_out"
            },
            "unit": "mM"
        },
        "V_cell": {
            "component": {
                "patV_cell1": "4/3*np.pi*r**3"
            },
            "unit": "fL"
        },
        "G": {
            "component": {
                "patG1": "4*np.pi*r**2"
            },
            "unit": "(um)^2"
        },
        "V_cyt": {
            "component": {
                "patV_cyt1": "0.5*V_cell"
            },
            "unit": "fL"
        },
        "V_nuc": {
            "component": {
                "patV_nuc1": "0.07*V_cell"
            },
            "unit": "fL"
        },
        "V_os": {
            "component": {
                "patV_os1": "4/3*np.pi*r_os**3"
            },
            "unit": "fL"
        },
        "pi_e": {
            "component": {
                "patpi_e1": "c_e*R*T"
            },
            "unit": "Pa"
        },
        "pi_i": {
            "component": {
                "patpi_i1": "c_i*R*T"
            },
            "unit": "Pa"
        },
        "V_ratio": {
            "component": {
                "patV_ratio2": "-G*Lp*(pi_e+pi_t-pi_i)/V_cell"
            },
            "unit": "s^-1"
        },
        "A_Ar": {
            "component": {
                "patA_Ar1": "R*T/c_strich_ATP*(ATP-c_strich_ATP)*(1+K_eq)"
            },
            "unit": "J mol^-1"
        },
        "J_H": {
            "component": {
                "patJ_H1": "R*T*(L_HH*np.log(H_in/H_out))",
                "patJ_H2": "+R*T*(L_HK*np.log(K_in/K_out))",
                "patJ_H3": "+R*T*(L_HNa*np.log(Na_in/Na_out))",
                "patJ_H4": "+R*T*(L_HCl*np.log(Cl_in/Cl_out))",
                "patJ_H5": "+F*Deltaphi*(L_HH+L_HK+L_HNa-L_HCl)",
                "patJ_H6": "-L_ArH*A_Ar"
            },
            "unit": "mol m^-2 s^-1"
        },
        "J_K": {
            "component": {
                "patJ_K1": "R*T*(L_KH*np.log(H_in/H_out))",
                "patJ_K2": "+R*T*(L_KK*np.log(K_in/K_out))",
                "patJ_K3": "+F*Deltaphi*(L_KH+L_KK)"
            },
            "unit": "mol m^-2 s^-1"
        },
        "J_Na": {
            "component": {
                "patJ_Na1": "R*T*(L_NaH*np.log(H_in/H_out))",
                "patJ_Na2": "+R*T*(L_NaNa*np.log(Na_in/Na_out))",
                "patJ_Na3": "+F*Deltaphi*(L_NaH+L_NaNa)"
            },
            "unit": "mol m^-2 s^-1"
        },
        "J_Cl": {
            "component": {
                "patJ_Cl1": "R*T*(L_ClH*np.log(H_in/H_out))",
                "patJ_Cl2": "+R*T*(L_ClCl*np.log(Cl_in/Cl_out))",
                "patJ_Cl3": "+F*Deltaphi*(L_ClH-L_ClCl)"
            },
            "unit": "mol m^-2 s^-1"
        },
        "Na_indt": {
            "component": {
                "patNa_indt2": "-(J_Na*G)/V_os*1e6",
                "patNa_indt3": "-Na_in*V_ratio"
            },
            "unit": "mM"
        },
        "Cl_indt": {
            "component": {
                "patCl_indt2": "-(J_Cl*G)/V_os*1e6",
                "patCl_indt3": "-Cl_in*V_ratio"
            },
            "unit": "mM"
        },
        "K_indt": {
            "component": {
                "patK_indt2": "-(J_K*G)/V_os*1e6",
                "patK_indt3": "-K_in*V_ratio"
            },
            "unit": "mM"
        },
        "H_indt": {
            "component": {
                "patH_indt2": "-(J_H*G*np.log(10)*H_in)/(V_os*pbc)*1e6",
                "patH_indt3": "-H_in*V_ratio"
            },
            "unit": "mM"
        },
        "Na_outdt": {
            "component": {
                "patNa_outdt1": "(J_Na*G)/V_out*1e6"
            },
            "unit": "mM"
        },
        "Cl_outdt": {
            "component": {
                "patCl_outdt1": "(J_Cl*G)/V_out*1e6"
            },
            "unit": "mM"
        },
        "K_outdt": {
            "component": {
                "patK_outdt1": "(J_K*G)/V_out*1e6"
            },
            "unit": "mM"
        },
        "H_outdt": {
            "component": {
                "patH_outdt1": "(J_H*G)/V_out*1e6"
            },
            "unit": "mM"
        },
        "R_ref": {
            "component": {
                "patR_ref1": "r/(1+(1-nu)*(pi_t*r)/(E*2*d))"
            },
            "unit": "um"
        },
        "V_ref": {
            "component": {
                "patV_ref1": "4/3*np.pi*R_ref**3"
            },
            "unit": "fL"
        },
        "R_refdt": {
            "component": {
                "patR_refdt1": "phi*R_ref*r/(2*d)*max(pi_t-pi_c,0)"
            },
            "unit": "um"
        },
        "r_osdt": {
            "component": {
                "patr_osdt2": "-Lp*pi_e",
                "patr_osdt3": "-Lp*pi_t",
                "patr_osdt4": "+Lp*pi_i"
            },
            "unit": "um"
        },
        "r_bdt": {
            "component": {
                "patr_bdt1": "0.2*R_refdt"
            },
            "unit": "um"
        },
        "rdt": {
            "component": {
                "patrdt1": "r_bdt",
                "patrdt2": "+r_osdt"
            },
            "unit": "um"
        },
        "n_totalHog1": {
            "component": {
                "patn_totalHog11": "(Hog1c*V_cyt+Hog1n*V_nuc+Hog1PPc*V_cyt+Hog1PPn*V_nuc)"
            },
            "unit": "mM fL"
        },
        "totalHog1PP": {
            "component": {
                "pattotalHog1PP1": "(Hog1PPc*V_cyt+Hog1PPn*V_nuc)/n_totalHog1"
            },
            "unit": "mM fL"
        }
    },
    "ODE": {
        "dL_ArH": {
            "component": {
                "patL_ArH1": "k_incrArH",
                "patL_ArH2": "-k_decrArH*L_ArH"
            },
            "condition": "if t >= t0_glucose and t < t1_glucose and Glucose == True else 0",
            "unit": "mol^2 J^-1 m^-2 s^-1"
        },
        "dL_HH": {
            "component": {
                "patL_HH1": "k_incrHH",
                "patL_HH2": "-k_decrHH*L_HH"
            },
            "condition": "if t >= t0_glucose and t < t1_glucose and Glucose == True else 0",
            "unit": "mol^2 J^-1 m^-2 s^-1"
        },
        "dATP": {
            "component": {
                "patATP1": "k_ATPincr",
                "patATP2": "-k_ATPdecr*ATP",
                "patATP3": "-ATP*V_ratio"
            },
            "condition": "if t >= t0_glucose and ATP < 2.5 and Glucose == True else - ATP * V_ratio",
            "unit": "mM"
        },
        "dDeltaphi": {
            "component": {
                "patDeltaphi2": "-(2*F/C_m)*(J_H+J_K+J_Na-J_Cl)"
            },
            "unit": "V"
        },
        "dPbs2": {
            "component": {
                "patPbs21": "-((k_phoPbs2*Pbs2)/(1+(pi_t/alpha)**hill_exponent))",
                "patPbs22": "+k_dephoPbs2*Pbs2PP",
                "patPbs23": "-Pbs2*V_ratio"
            },
            "unit": "mM"
        },
        "dPbs2PP": {
            "component": {
                "patPbs2PP1": "((k_phoPbs2*Pbs2)/(1+(pi_t/alpha)**hill_exponent))",
                "patPbs2PP2": "-k_dephoPbs2*Pbs2PP",
                "patPbs2PP3": "-Pbs2PP*V_ratio"
            },
            "unit": "mM"
        },
        "dHog1c": {
            "component": {
                "patHog1c2": "-k_phoHog1*Pbs2PP*Hog1c",
                "patHog1c3": "+k_dephoHog1PPc*Hog1PPc",
                "patHog1c4": "-k_impHog1c*Hog1c",
                "patHog1c5": "+((k_expHog1n*Hog1n*V_nuc)/V_cyt)",
                "patHog1c6": "-Hog1c*V_ratio"
            },
            "unit": "mM"
        },
        "dHog1PPc": {
            "component": {
                "patHog1PPc1": "k_phoHog1*Pbs2PP*Hog1c",
                "patHog1PPc2": "-k_dephoHog1PPc*Hog1PPc",
                "patHog1PPc3": "-k_impHog1PPc*Hog1PPc",
                "patHog1PPc4": "+((k_expHog1PPn*Hog1PPn*V_nuc)/V_cyt)",
                "patHog1PPc5": "-Hog1PPc*V_ratio"
            },
            "unit": "mM"
        },
        "dHog1n": {
            "component": {
                "patHog1n1": "((k_impHog1c*Hog1c*V_cyt)/V_nuc)",
                "patHog1n2": "-k_expHog1n*Hog1n",
                "patHog1n3": "+k_dephoHog1PPn*Hog1PPn",
                "patHog1n4": "-Hog1n*V_ratio"
            },
            "unit": "mM"
        },
        "dHog1PPn": {
            "component": {
                "patHog1PPn1": "((k_impHog1PPc*Hog1PPc*V_cyt)/V_nuc)",
                "patHog1PPn2": "-k_expHog1PPn*Hog1PPn",
                "patHog1PPn3": "-k_dephoHog1PPn*Hog1PPn",
                "patHog1PPn4": "-Hog1PPn*V_ratio"
            },
            "unit": "mM"
        },
        "dGlyc_in": {
            "component": {
                "patGlyc_in1": "k_s0Glyc",
                "patGlyc_in2": "+((k_s1Glyc*totalHog1PP**4)/(beta**4+totalHog1PP**4))",
                "patGlyc_in3": "+k_s2Glyc*Yt",
                "patGlyc_in4": "-(k_exp0Glyc+((k_exp1Glyc*pi_t**hill_exponent_eflux)/(gamma**hill_exponent_eflux+pi_t**hill_exponent_eflux)))*(Glyc_in-Glyc_ex)",
                "patGlyc_in5": "-Glyc_in*V_ratio"
            },
            "unit": "mM"
        },
        "dYt": {
            "component": {
                "patYt1": "k_s0Yt",
                "patYt2": "+k_s1Yt*z4",
                "patYt3": "-k_tYt*Yt",
                "patYt4": "-Yt*V_ratio"
            },
            "unit": "mM"
        },
        "dz1": {
            "component": {
                "patz11": "((4*(Hog1PPn-z1))/tau)"
            },
            "unit": "mM"
        },
        "dz2": {
            "component": {
                "patz21": "((4*(z1-z2))/tau)"
            },
            "unit": "mM"
        },
        "dz3": {
            "component": {
                "patz31": "((4*(z2-z3))/tau)"
            },
            "unit": "mM"
        },
        "dz4": {
            "component": {
                "patz41": "((4*(z3-z4))/tau)"
            },
            "unit": "mM"
        },
        "dpi_t": {
            "component": {
                "patpi_t1": "E*2*d/(1-nu)*rdt/(r**2)",
                "patpi_t2": "-E*2*d/(1-nu)*R_refdt/(R_ref*r)",
                "patpi_t3": "-rdt/r*pi_t"
            },
            "unit": "Pa"
        },
        "dR_ref": {
            "component": {
                "patR_ref1": "R_refdt"
            },
            "unit": "um"
        },
        "dr_os": {
            "component": {
                "patr_os1": "r_osdt"
            },
            "unit": "um"
        },
        "dr_b": {
            "component": {
                "patr_b1": "r_bdt"
            },
            "unit": "um"
        },
        "dr": {
            "component": {
                "patr1": "rdt"
            },
            "unit": "um"
        },
        "dc_i": {
            "component": {
                "patc_i1": "Na_indt",
                "patc_i2": "+K_indt",
                "patc_i3": "+Cl_indt",
                "patc_i4": "+H_indt",
                "patc_i5": "+dGlyc_in",
                "patc_i6": "+(k_uptake*G-k_consumption*V_cell)/V_cell*1e18",
                "patc_i7": "-c_i*V_ratio"
            },
            "unit": "mM"
        },
        "dH_in": {
            "component": {
                "patH_in1": "H_indt"
            },
            "unit": "mM"
        },
        "dCl_in": {
            "component": {
                "patCl_in1": "Cl_indt"
            },
            "unit": "mM"
        },
        "dCl_out": {
            "component": {
                "patCl_out1": "Cl_outdt"
            },
            "unit": "mM"
        },
        "dH_out": {
            "component": {
                "patH_out1": "H_outdt"
            },
            "unit": "mM"
        },
        "dK_in": {
            "component": {
                "patK_in1": "K_indt"
            },
            "unit": "mM"
        },
        "dK_out": {
            "component": {
                "patK_out1": "K_outdt"
            },
            "unit": "mM"
        },
        "dNa_in": {
            "component": {
                "patNa_in1": "Na_indt"
            },
            "unit": "mM"
        },
        "dNa_out": {
            "component": {
                "patNa_out1": "Na_outdt"
            },
            "unit": "mM"
        },
        "dSorbitol_out": {
            "component": {
                "patSorbitol_out1": "0"
            },
            "unit": "mM"
        }
    }
}